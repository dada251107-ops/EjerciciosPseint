Algoritmo sin_titulo
	//definir tipo de variables
	Definir Nombre_aprendiz Como Cadena;
	Definir Apellido_aprendiz como cadena
	Definir saludo Como Cadena;
	Escribir "hola--- ";
	//2. tomar datos o asignacion de variables
	Nombre_aprendiz <-"david_";
	Nombre_aprendiz <-"arias"
	Leer Nombre_aprendiz
	Leer Apellido_aprendiz
	saludo <- "Bienvenido ... " + (Nombre_aprendiz) + (Apellido_aprendiz);
	//4. Mostrar resultados
	Escribir  saludo;
FinAlgoritmo
